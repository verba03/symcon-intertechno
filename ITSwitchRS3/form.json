                },
                {
                    "name": "CharAdr",
                    "type": "Select",
                    "caption": "Letter",
                    "options": [
                        {
                            "label": "A",
                            "value": "1FFF"
                        },
                        {
                            "label": "B",
                            "value": "F1FF"
                        },
                        {
                            "label": "C",
